<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
        package="com.tencent.multiadapter">

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".example.ui.MultiDataTypeDemoActivity"></activity>
        <activity android:name=".example.ui.ViewBindingActivity" />
        <activity android:name=".example.ui.DiffUtilDemoActivity" />
        <activity android:name=".example.ui.SingleCheckActivity" />
        <activity android:name=".example.ui.CheckItemActivity" />
        <activity android:name=".example.ui.MultiItemActivity" />
        <activity android:name=".example.ui.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>